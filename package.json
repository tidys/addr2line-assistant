{
  "name": "addr2line-assistant",
  "displayName": "addr2line-assistant",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.79.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:addr2line-assistant.helloWorld"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "configuration": {
      "title": "",
      "properties": {
        "a2la.phones": {
          "type": "array",
          "default": [],
          "description": "连接的手机"
        },
        "a2la.apps": {
          "type": "array",
          "default": [],
          "description": "游戏包名"
        },
        "a2la.leak-file": {
          "type": "string",
          "default": "leak-report.txt",
          "description": "内存泄露报告文件"
        }
      }
    },
    "commands": [
      {
        "command": "addr2line-assistant.helloWorld",
        "title": "Hello World"
      },
      {
        "command": "addr2line-assistant.addPhone",
        "title": "addPhone",
        "icon": "$(add)"
      },
      {
        "command": "addr2line-assistant.removePhone",
        "title": "removePhone",
        "icon": "$(remove)"
      },
      {
        "command": "addr2line-assistant.connectPhone",
        "title": "connectPhone",
        "icon": "$(debug-disconnect)"
      }
    ],
    "viewsContainers": {
      "activitybar": [
        {
          "id": "addr2line",
          "title": "addr2line",
          "icon": "./address.svg"
        }
      ]
    },
    "menus": {
      "view/title": [
        {
          "command": "addr2line-assistant.addPhone",
          "when": "view == addr2line:main",
          "group": "navigation"
        }
      ],
      "view/item/context": [
        {
          "command": "addr2line-assistant.removePhone",
          "when": "view == addr2line:main",
          "group": "inline"
        },
        {
          "command": "addr2line-assistant.connectPhone",
          "when": "view== addr2line:main",
          "group": "inline"
        }
      ]
    },
    "views": {
      "addr2line": [
        {
          "id": "addr2line:main",
          "name": "leak with app"
        },
        {
          "id": "addr2line:main2",
          "name": "leak with file"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^8.0.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "16.x",
    "@types/vscode": "^1.79.0",
    "@typescript-eslint/eslint-plugin": "^5.45.0",
    "@typescript-eslint/parser": "^5.45.0",
    "@vscode/test-electron": "^2.2.0",
    "eslint": "^8.28.0",
    "glob": "^8.0.3",
    "mocha": "^10.1.0",
    "typescript": "^4.9.3"
  },
  "dependencies": {
    "adb-driver": "^0.1.8"
  }
}
